{
  "name": "cssobj-plugin-flexbox",
  "version": "1.0.2",
  "description": "cssobj plugin for generate right flexbox CSS, auto vendor prefixer for flexbox in browser, input one line, give you right flexbox!",
  "main": "dist/cssobj-plugin-flexbox.cjs.js",
  "jsnext:main": "dist/cssobj-plugin-flexbox.es.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "istanbul cover _mocha",
    "build": "rollup -c",
    "make": "gulp bump && npm run build",
    "codecov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec && ./node_modules/.bin/codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cssobj/cssobj-plugin-flexbox.git"
  },
  "keywords": [
    "cssobj",
    "plugin",
    "flexbox"
  ],
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.2.0",
    "mocha": "^2.5.3",
    "rollup": "^0.33.1",
    "rollup-plugin-replace": "^1.1.1",
    "uglify-js": "^2.7.3"
  },
  "author": "James Yang <jamesyang999@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cssobj/cssobj-plugin-flexbox/issues"
  },
  "homepage": "https://github.com/cssobj/cssobj-plugin-flexbox#readme"
}
